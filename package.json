{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "turbo": "^2.3.4",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.42",
  "workspaces": [
    "apps/*",
    "apps/stackblitz/angular/concepts/*",
    "apps/stackblitz/angular/examples/*",
    "apps/stackblitz/react/concepts/*",
    "apps/stackblitz/react/examples/*",
    "packages/*"
  ]
}
